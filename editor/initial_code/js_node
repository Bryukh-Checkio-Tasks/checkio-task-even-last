"use strict";

function checkio(data) {
    return data;
}

var assert = require('assert');

if (!global.is_checking) {
    assert.equal(checkio([0, 1, 2, 3, 4, 5]), 30, "(0+2+4)*5=30");
    assert.equal(checkio([1, 3, 5]), 30, "(1+5)*5=30");
    assert.equal(checkio([6]), 36, "(6)*6=36");
    assert.equal(checkio([]), 0, "An empty array = 0");
    console.log("Coding complete? Click 'Check' to review your tests and earn cool rewards!");
}
