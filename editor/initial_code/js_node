import assert from "assert";

function evenLast(data: number[]): number {
    return 0;
}


console.log('Example:')
console.log(evenLast([0, 1, 2, 3, 4, 5]))

assert.equal(evenLast([0, 1, 2, 3, 4, 5]), 30);
assert.equal(evenLast([1, 3, 5]), 30);
assert.equal(evenLast([6]), 36);
assert.equal(evenLast([]), 0);
